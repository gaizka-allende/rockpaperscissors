{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
            "jsx": true
		}
	},
	"rules": {
		"no-console": 0
	},
    "plugins": ["react", "jsx"]
}
